{
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    },
    "csharp-ls": {
      "command": "csharp-ls",
      "filetypes": ["cs"],
      "rootPatterns": ["*.csproj", ".git/"]
    }
  },
  "yaml.schemas": {
    "kubernetes": ["deployment.ya?ml", "service.ya?ml", "ingress.ya?ml"],
    "https://json.schemastore.org/circleciconfig.json": ".circleci/config.ya?ml",
    "https://raw.githubusercontent.com/aquaproj/aqua/main/json-schema/aqua-yaml.json": [
      "aqua.yml",
      "aqua.yaml"
    ],
    "https://raw.githubusercontent.com/aquaproj/aqua/main/json-schema/registry.json": [
      "registry.yaml"
    ],
    "https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json": "/docker-compose.ya?ml",
    "https://raw.githubusercontent.com/awslabs/goformation/master/schema/cloudformation.schema.json": [
      "*cfn*.yaml",
      "*cfn*.yml"
    ],
    "https://json.schemastore.org/github-workflow.json": [
      ".github/workflows/*.ya?ml"
    ]
  },
  "yaml.schemaStore.enable": false,
  "yaml.completion": true,
  "yaml.validate": true,
  "typescript.suggestionActions.enabled": false,
  "javascript.suggestionActions.enabled": false,
  "javascript.validate.enable": false,
  "coc.preferences.formatOnSave": true,
  "json.validate.enable": true,
  "json.schemas": [
    {
      "fileMatch": ["default.json"],
      "url": "https://docs.renovatebot.com/renovate-schema.json"
    }
  ],
  "go.goplsPath": "gopls",
  "go.goplsOptions": {
    "gofumpt": true
  }
}
